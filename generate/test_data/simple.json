{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "height": {
      "type": "integer"
    },
    "width": {
      "type": "number"
    },
    "visible": {
      "type": "boolean"
    },
    "someDateObj": {
      "type": "object",
      "properties": {
        "dates": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    }
  },
  "required": [
    "type"
  ]
}

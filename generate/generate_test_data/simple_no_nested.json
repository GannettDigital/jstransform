{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "height": {
      "type": "integer"
    },
    "width": {
      "type": "number"
    },
    "visible": {
      "type": "boolean"
    },
    "someDateObj": {
      "type": "object",
      "properties": {
        "dates": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "aMap": {
      "type": "object",
      "additionalProperties": true,
      "properties": {}
    },
    "contributors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "contributorId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "name"
        ]
      }
    }
  },
  "required": [
    "type"
  ]
}
